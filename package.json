{
  "name": "otak-proxy",
  "displayName": "otak-proxy",
  "description": "Switch proxy settings for VSCode, Git, and GitHub CLI with one click. Enhance your workflow with a simple, efficient tool.",
  "publisher": "odangoo",
  "license": "MIT",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/tsuyoshi-otake-system-exe-jp/otak-proxy"
  },
  "bugs": {
    "url": "https://github.com/tsuyoshi-otake-system-exe-jp/otak-proxy/issues"
  },
  "homepage": "https://github.com/tsuyoshi-otake-system-exe-jp/otak-proxy#readme",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.9.0"
  },
  "categories": [
    "Other",
    "Education"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "otak-proxy.toggleProxy",
        "title": "Toggle Proxy Settings"
      }
    ],
    "configuration": {
      "title": "otak-proxy",
      "properties": {
        "otakProxy.proxyUrl": {
          "type": "string",
          "default": "",
          "description": "Proxy server URL (e.g., http://proxy.example.com:8080)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.9.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
